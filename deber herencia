class miClase:
    x=5
    
p1 = miClase()
print(p1.x) 

class persona:
    nombre = str
    edad = int
    
    def __init__(self,nombre,edad):
        self.nombre = nombre
        self.edad = edad 
    def __str__(self):
        return f"Su nombre es {self.nombre}, y su edad es {self.edad}." 
       
p2 = persona("Diego Alexandar", 29)

print(p2)

class Persona2:
    nombre = str
    edad = int
    cedula = int
  
    def __init__(self, nombre, edad, cedula):
        self.nombre = nombre
        self.edad = edad
        self.cedula = cedula
    
    def __str__(self):
        return f"Hola mi nombre es  {self.nombre} y tengo {self.edad} años y mi numero de cédula es el: {self.cedula}"   
        
p3 = Persona2 ("Bryan", 19, 1726375767)
p3.nombre = "Steven"
p3.cedula = "1726375766"

print(p3)

class persona3:
    nombre = str
    edad = int
    altura = int
    
    def __init__(self, nombre, edad, estatura):
        self.nombre = nombre
        self.edad = edad 
        self.estatura= estatura
    def __str__(self):
        return f"Su nombre es {self.nombre} y tu edad es {self.edad} y tu estaruta es de: {self.estatura}"
       
p4 = persona3 ("Jose", 19, 1.67)

print(p4)
